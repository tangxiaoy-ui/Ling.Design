{
  "compilerOptions": {
    "target": "es2019",
    "module": "esnext",
    "lib": ["dom", "esnext"],
    "declaration": true,
    "declarationMap": true,
    "moduleResolution": "node",
    "jsx": "react-jsx",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "baseUrl": "."
  },
  "include": ["src"]
}
